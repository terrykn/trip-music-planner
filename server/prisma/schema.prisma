// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String   @unique
  email     String   @unique
  password  String 
  genre     String 

}

model Path {
  id           Int    @id @default(autoincrement())
  locationFrom String
  locationTo   String
  time         Int  
}

model Music {
  id         Int      @id @default(autoincrement())
  albumName  String 
  genre      String
  artistName String 
  duration   Int      
}
